{
	"update_url": "https://clients2.google.com/service/update2/crx",
	"manifest_version": 3,
	"name": "ctrl-scroll",
	"version": "1.3",
	"description": "Scroll 5x the normal amount on Ctrl+Wheel",
	"homepage_url": "https://github.com/gushogg-blake/chrome-ctrl-scroll",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"run_at": "document_start",
			"all_frames": true,
			"js": ["src/ctrl-scroll.js"]
		}
	]
}
